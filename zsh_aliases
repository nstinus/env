alias addalias='source ~/scripts/addalias.sh'
alias backup='source ~/scripts/backup.sh'
alias cp='nocorrect cp'       # no spelling correction on cp
alias cursha='python ~/scripts/microtimestamp.py | openssl dgst -sha1'
alias d='dirs -v'
alias delalias='source  ~/scripts/delalias.sh'
alias duX='du -s ./* | sort -n'
alias envg='env | grep -i'
alias gdev='cd ~/projects/dev/'
alias gdlc='python /Users/nico/scripts/git_diff_line_counter.py'
alias gdown='cd ~/Downloads'
alias gpl='python2.5 /Users/nico/scripts/git_plot_lines.py'
alias gsandbox='cd ~/projects/dev/sandbox'
alias gstat='git status'
alias h=history
alias histo='python2.5 ~/scripts/histogramizer.py'
alias j=jobs
alias load='python ~/scripts/load.py'
alias lrt='ls -lrt'
alias lrtg='ls -lrt | grep --color -i'
alias lrth='ls -lrth'
alias mkdir='nocorrect mkdir' # no spelling correction on mkdir
alias mv='nocorrect mv'       # no spelling correction on mv
alias nsp='new_sandbox_project'
alias po=popd
alias psg='ps -ef | grep -v -E ".*$UID.*grep" | grep --color -i'
alias restore='python ~/scripts/restore.py'
alias ts='date +%y-%m-%d_%H:%M:%S'
alias uts='date +%s'
alias mount_usb="sudo mount /dev/sdb1 /mnt/usbstick"
alias umount_usb="sudo umount /mnt/usbstick"
